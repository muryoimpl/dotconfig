[
  {
    "output": ["DP-1", "HDMI-A-1"],
    "layer": "top", // Waybar at top layer
    "position": "top", // Waybar position (top|bottom|left|right)
    "height": 30, // Waybar height (to be removed for auto height)
    "spacing": 1, // Gaps between modules (4px)
    "margin": 0,
    "modules-left": [
      "ext/workspaces",
      "tray"
    ],
    "modules-center": [
      "dwl/window"
    ],
    "modules-right": ["backlight", "pulseaudio", "network", "cpu", "memory", "disk", "temperature", "clock"],
    "ext/workspaces": {
      "format": "{icon}",
      "format-icons": {
          "urgent": "ÔÅ™",
          "focused": "ÔÜí",
          "default": ""
      },
      "on-click": "activate",
      "on-click-right": "deactivate",
      "sort-by-number": true,
      "all-outputs": true,
      "persistent_workspaces": {
        "1": [],
        "2": [],
        "3": [],
        "4": [],
        "5": []
      },
    },
    "dwl/window": {
        "format": "[{layout}]{title}"
    },
    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "ÔÅÆ",
            "deactivated": "ÔÅ∞"
        },
        "tooltip": true,
    },
    "tray": {
        "icon-size": 18,
        "spacing": 5
    },
    "clock": {
        "timezone": "Asia/Tokyo",
        "tooltip": true,
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "format": "{:%Y-%m-%d(%a) %H:%M}",
        "format-alt": "{:%Y-%m-%d(%a) %H:%M}",
        "interval": 1,
        "locale": "ja_JP.UTF-8",
        "calendar": {
            "start-day": "sunday",
            "mode"          : "year",
            "mode-mon-col"  : 3,
            "weeks-pos"     : "",
            "on-scroll"     : 1,
            "on-click-right": "mode",
            "format": {
                "months":     "<span color='#d3c6aa'><b>{}</b></span>",
                "days":       "<span color='#e67e80'>{}</span>",
                "weeks":      "<span color='#a7c080'><b>W{}</b></span>",
                "weekdays":   "<span color='#7fbbb3'><b>{}</b></span>",
                "today":      "<span color='#dbbc7f'><b><u>{}</u></b></span>"
            }
        }
    },
    "cpu": {
        "format": "CPU:{usage}%",
        "interval": 1,
        "tooltip": true
    },
    "memory": {
        "format": "Mem: {}%",
        "interval": 1,
        "tooltip": true
    },
    "temperature": {
        // "thermal-zone": 2,
        "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
        "critical-threshold": 80,
        // "format-critical": "{temperatureC}¬∞C {icon}",
        "format": "M.2: {icon} {temperatureC}¬∞C",
        "format-icons": ["üÜí", "üå°Ô∏è", "ü•µ"]
    },
    "network": {
        // "interface": "wlp2*", // (Optional) To force the use of this interface
        "format-wifi": "Ôá´ {essid} ({signalStrength}%)",
        "format-ethernet": "üåê {ipaddr}/{cidr}",
        "tooltip-format": "üñß {ifname} via {gwaddr}",
        "format-linked": "üñß {ifname} (No IP)",
        "format-disconnected": " ‚ö† Disconnected",
        "format-alt": "{ifname}: {ipaddr}/{cidr}"
    },
    "pulseaudio": {
        // "scroll-step": 1, // %, can be a float
        "format": "{volume}% {icon} {format_source}",
        "format-bluetooth": "{volume}% {icon}·öº·õí{format_source}",
        "format-bluetooth-muted": "üîá{icon}·öº·õí{format_source}",
        "format-muted": "üîá {format_source}",
        "format-source": "{volume}% ÔÑ∞",
        "format-source-muted": "ÔÑ±",
        "format-icons": {
            "headphone": "üéß",
            "hands-free": "üéß",
            "headset": "üéß",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
        },
        "on-click": "pavucontrol"
    },
    "disk": {
      "interval": 30,
      "format": "Disk: {percentage_used}%",
      "path": "/"
    },
    "backlight": {
      // "device": "intel_backlight",
      "format": "{percent}% {icon}",
      "format-icons": ["ÔÜÖ", "ÔÑë"],
      "on-scroll-up": "brightnessctl set +5%",
      "on-scroll-down": "brightnessctl set 5%-"
    }
  },
